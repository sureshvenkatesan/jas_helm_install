# Environment variables for JFrog Platform installation on AWS EKS with S3 external storage and Postgres setup
export JFROG_PLATFORM_NAMESPACE=ps-jfrog-platform
export JFROG_PLATFORM_CHART=jfrog/jfrog-platform

export JFROG_PLATFORM_CHART_VERSION=11.2.5
export GLOBAL_RT_VERSION=7.111.4
export XRAY_VERSION=3.111.25

#=========================================================================
#Different configs below for all JPDs:

# JFROG_PLATFORM_NAME is the Helm release name
export JFROG_PLATFORM_NAME=example-release
export GCP_BUCKET_NAME=sureshv-ps-example
# Generate a random join key with 'openssl rand -hex 32'
export RT_MASTER_KEY=02ba23e285e065d2a372b889ac3dbd51510dd0399875f95294312634f50b6960
export JOIN_KEY=763d4bdf02ff4cc16d7c5cf2abeccf3f243b5557bf738ec5438fd55df0cec3cc
